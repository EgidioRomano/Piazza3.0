{
  "url": {
    "api": "https://api.citizenos.com",
    "fe": "https://app.citizenos.com"
  },
  "db": {
    "url": "postgres://citizenos:citizenos@localhost:5432/citizenos",
    "options": {
      "dialect": "postgres",
      "logging": true,
      "benchmark": true,
      "sync": {
        "force": false,
        "logging": true
      },
      "define": {
        "underscored": false,
        "freezeTableName": false,
        "syncOnAssociation": false,
        "charset": "utf8",
        "collate": "utf8_general_ci",
        "timestamps": true,
        "paranoid": true
      },
      "omitNull": false,
      "dialectOptions": {
        "ssl": {
          "require": true,
          "rejectUnauthorized": false
        }
      },
      "pool": {
        "min": 0,
        "max": 100
      },
      "isolationLevel": "READ COMMITTED"
    }
  },
  "topic": {
    "titleLengthMax": 1000
  },
  "session": {
    "name": "app.sid",
    "privateKey": "TO BE OVERWRITTEN IN ENV SPECIFIC CONF",
    "publicKey": "TO BE OVERWRITTEN IN ENV SPECIFIC CONF",
    "algorithm": "RS256",
    "secret": "sdfsdfsdfsdfdsfdsf",
    "resave": false,
    "saveUninitialized": false,
    "cookie": {
      "path": "/api",
      "httpOnly": true,
      "maxAge": 12096000,
      "domain": false,
      "sameSite": "None"
    }
  },
  "api": {
