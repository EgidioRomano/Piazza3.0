  "logging": {
    "log4js": {
      "appenders": {
        "file": {
          "type": "dateFile",
          "filename": "logs/app.log",
          "pattern": "-dd",
          "alwaysIncludePattern": false
        },
        "console": {
          "type": "console"
        }
      },
      "categories": {
        "default": {
          "appenders": [
            "file",
            "console"
          ],
          "level": "info"
        }
      }
    },
    "morgan": {
      "format": ":remote-addr - :remote-user \":method :url HTTP/:http-version\" :status :res[content-length] - :response-time ms"
    }
  },
  "attachments": {
    "limit": 10
  },
  "storage": {
    "allowedFileTypes": ["txt", "pdf", "doc", "docx", "ddoc", "bdoc", "asice", "odf", "odt", "jpg", "jpeg", "png", "rtf", "xls", "xlsx", "ppt", "pptx", "pps", "xlt"],
